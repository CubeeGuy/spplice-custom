{
  "applicationId": "com.p2r3.spplice",
  "version": "0.4.2",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "computer.*",
    "window.*",
    "filesystem.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "Spplice",
      "width": 800,
      "height": 500,
      "minWidth": 800,
      "minHeight": 500,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "spplice",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}
